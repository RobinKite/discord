{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"watch": {
		"stylelint-dev": {
			"patterns": [
				"/src/**/*.scss"
			],
			"extensions": "scss"
		},
		"eslint": {
			"patterns": [
				"/src/**/*.js"
			],
			"extensions": "js"
		}
	},
	"scripts": {
		"start": "npm-run-all -p watch-sass-js start-js vite",
		"build": "npm run eslint && npm run stylelint-build && react-scripts build",
		"eslint": "eslint 'src/**/*.js'",
		"start-js": "react-scripts start",
		"stylelint-fix-sass": "stylelint --fix 'src/**/*.scss'",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache'; exit 0",
		"stylelint-build": "stylelint 'src/**/*.scss'",
		"watch-sass-js": "npm-watch"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.5",
		"npm-run-all": "^4.1.5",
		"npm-watch": "^0.3.0",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.2",
		"react-router-dom": "^6.15.0",
		"sass": "^1.66.1",
		"stylelint": "^15.10.3",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-order": "^6.0.3"
	},
	"devDependencies": {
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@vitejs/plugin-react": "^4.0.3",
		"eslint": "^8.45.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"vite": "^4.4.5"
	}
}
